            "styleUrlsData": "",
            "stylesData": "",
            "extends": [],
            "templateData": "<div\r\n  class=\"bd-form-container\"\r\n  [style.columnGap.px]=\"columnGap\"\r\n  [style.rowGap.px]=\"rowGap\"\r\n  [style.padding.px]=\"padding\"\r\n  [style.paddingRight.px]=\"padding - columnGap\"\r\n>\r\n  <ng-container *ngFor=\"let el of elements\">\r\n    <div\r\n      class=\"bd-form-item\"\r\n      [style.width]=\"\r\n        'calc(' + el.colspan + ' / ' + columns + '* 100% - ' + columnGap + 'px)'\r\n      \"\r\n    >\r\n      <bd-form-element\r\n        [control]=\"formGroup | control : el.key\"\r\n        [label]=\"el.label!\"\r\n        [hintLeft]=\"el.hintLeft!\"\r\n        [hintRight]=\"el.hintRight!\"\r\n        [type]=\"el.type!\"\r\n      ></bd-form-element>\r\n    </div>\r\n  </ng-container>\r\n</div>\r\n"
